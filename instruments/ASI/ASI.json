{
    "src":    "instruments/ASI/ASI.svg",

    "animations": [
      {
        "element": "#Needle",
        "type":    "transform",
        "transforms": [{
          "type": "rotate",
          "a": {
            "property": "ias",
            "interpolation": "instruments/ASI/AirspeedInterpolation.xml"
          },
          "x": 256,
          "y": 256
        }]
      },
      {
        "element": "#TAS",
        "type":    "transform",
        "transforms": [{
          "type": "rotate",
              "a": {
            "property": "tas",
            "scale": -1
          },
          "x": 256,
          "y": 256
        }]
      }
    ]
}
